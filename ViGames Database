Created a Dataabse to figuring out what games sold the most copies over the years.

CREATE DATABASE Games

CREATE TABLE Games.Video_Games

INSERT INTO Games.Games
  (published_yr, Game_titles, Publisher, Copy_sold)
	VALUES
  (2011,'Minecraft','Mojang Studios',300000000),
	(2013, 'Grand Theft Auto', 'Rockstar North', 195000000),
	(2006, 'Tetris (EA)', 'EA Mobile', 100000000),
	(2006, 'Wii Sports', 'Nintendo EAD', 82900000),
	(2017, 'PUBG: Battleground', 'PUBG Studios', 75000000),
	(2014, 'Mario Kart 8/ Deluxe', 'Nintendo EAD/Nintendo EPD', 69040000),
	(2018, 'Red Dead Redemption 2', 'Rockstar Games', 61000000),
	(1984, 'Super Mario Bros.', 'Nintendo R&D4', 58000000),
	(2016, 'Overwatch', 'Blizzard Entertainment', 50000000),
	(2016, 'Human: Fall Flat', 'No Brakess, Game', 50000000);

